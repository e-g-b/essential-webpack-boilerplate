{
  "name": "essencial-webpack-boilerplate",
  "version": "3.0.0",
  "description": "Minimal starter boilerplate with WebPack 4, PostCSS 6, Babel and Jest ready to go",
  "main": "./src/index.js",
  "repository": {
    "url": "https://github.com/sandrina-p/essencial-webpack-boilerplate.git",
    "type": "git"
  },
  "author": "Sandrina Pereira <sandrin4pereir4@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production --config ./config/webpack.config.js",
    "start": "webpack-dev-server --mode development --config ./config/webpack.config.js",
    "test": "cross-env NODE_ENV=test node --harmony_proxies node_modules/jest/bin/jest.js --coverage",
    "test:node7": "cross-env NODE_ENV=test node --harmony node_modules/jest/bin/jest.js --coverage"
  },
  "devDependencies": {
    "ajv-keywords": "^3.1.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^2.0.1",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^3.0.4",
    "jest": "^22.4.2",
    "lodash-webpack-plugin": "^0.11.4",
    "postcss": "^6.0.14",
    "postcss-css-variables": "^0.8.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-nested": "^3.0.0",
    "style-loader": "^0.20.2",
    "stylelint": "9.x.x",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
